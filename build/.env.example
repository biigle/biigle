GITHUB_OAUTH_TOKEN=

APP_NAME=BIIGLE
APP_ENV="production"
APP_KEY=
APP_DEBUG=false
APP_LOG=daily
APP_LOG_LEVEL="debug"
APP_URL="https://example.com"
APP_TIMEZONE="Europe/Berlin"

ADMIN_EMAIL="info@example.com"

VOLUME_ADMIN_STORAGE_DISKS=local
VOLUME_EDITOR_STORAGE_DISKS=

DB_CONNECTION="pgsql"
DB_HOST="database"
DB_PORT=5432
DB_DATABASE="biigle"
DB_USERNAME="biigle"
DB_PASSWORD="secret"

# if your server is behind a proxy, uncomment and set this
#HTTP_PROXY=host:port

# normally you don't have to edit these
LARGO_PATCH_STORAGE_DISK=largo
REPORTS_STORAGE_DISK=reports
GEO_OVERLAY_STORAGE_DISK=geo-overlays
SYNC_IMPORT_STORAGE_DISK=imports
LASERPOINTS_STORAGE_DISK=laserpoints
MAIA_ANNOTATION_CANDIDATE_STORAGE_DISK=maia-ac
MAIA_TRAINING_PROPOSAL_STORAGE_DISK=maia-tp
VIDEOS_THUMBNAIL_STORAGE_DISK=video-thumbs

MAIA_REQUEST_QUEUE=gpu
MAIA_REQUEST_CONNECTION=gpu
MAIA_RESPONSE_QUEUE=default
MAIA_RESPONSE_CONNECTION=redis
MAIA_AVAILABLE_BYTES=15E+9
MAIA_MAX_WORKERS=10

CACHE_DRIVER="redis"
SESSION_DRIVER="redis"
SESSION_LIFETIME=120
QUEUE_DRIVER="redis"

REDIS_HOST="cache"
REDIS_PASSWORD=null
REDIS_PORT=6379

# see config/mail.php for what drivers are available
# default is the PHP mail function which doesn't require any credentials
MAIL_DRIVER="mail"
MAIL_HOST="smtp.mailtrap.io"
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_ADDRESS="noreply@example.com"
