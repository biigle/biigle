GITHUB_OAUTH_TOKEN=

APP_NAME=BIIGLE
APP_ENV="production"
APP_KEY=
APP_DEBUG=false
APP_URL="https://example.com"
APP_TIMEZONE="UTC"

LOG_CHANNEL=daily
LOG_LEVEL="debug"

ADMIN_EMAIL="info@example.com"

VOLUME_ADMIN_STORAGE_DISKS=local
VOLUME_EDITOR_STORAGE_DISKS=

DB_CONNECTION="pgsql"
DB_HOST="database"
DB_PORT=5432
DB_DATABASE="biigle"
DB_USERNAME="biigle"
DB_PASSWORD="secret"

BROADCAST_DRIVER=pusher
PUSHER_HOST=websockets
PUSHER_PORT=6001
PUSHER_APP_ID=biigle
PUSHER_APP_KEY=biigle
PUSHER_APP_SECRET=

SOKETI_DEFAULT_APP_ID="${PUSHER_APP_ID}"
SOKETI_DEFAULT_APP_KEY="${PUSHER_APP_KEY}"
SOKETI_DEFAULT_APP_SECRET="${PUSHER_APP_SECRET}"

# if your server is behind a proxy, uncomment and set this
#HTTP_PROXY=host:port

# normally you don't have to edit these
LARGO_PATCH_STORAGE_DISK=largo
REPORTS_STORAGE_DISK=reports
GEO_OVERLAY_STORAGE_DISK=geo-overlays
SYNC_IMPORT_STORAGE_DISK=imports
LASERPOINTS_STORAGE_DISK=laserpoints
MAIA_ANNOTATION_CANDIDATE_STORAGE_DISK=maia-ac
MAIA_TRAINING_PROPOSAL_STORAGE_DISK=maia-tp
VIDEOS_THUMBNAIL_STORAGE_DISK=video-thumbs
MAGIC_SAM_EMBEDDING_STORAGE_DISK=magic-sam

MAIA_REQUEST_QUEUE=gpu
MAIA_REQUEST_CONNECTION=gpu
MAIA_RESPONSE_QUEUE=default
MAIA_RESPONSE_CONNECTION=redis
MAIA_FEATURE_VECTOR_QUEUE=gpu
MAIA_FEATURE_VECTOR_CONNECTION=gpu
MAIA_MAX_WORKERS=10
MAIA_PYTHON="/opt/conda/bin/python3"

MAGIC_SAM_REQUEST_QUEUE=gpu
MAGIC_SAM_REQUEST_CONNECTION=gpu
MAGIC_SAM_DEVICE=cuda
MAGIC_SAM_PYTHON="/opt/conda/bin/python3"

PTP_REQUEST_QUEUE=gpu
PTP_REQUEST_CONNECTION=gpu
PTP_PYTHON="/opt/conda/bin/python3"

# Set to false until the LabelBOT database index is generated.
LABELBOT_SHOW_BUTTON=true
# Add comma-separated IDs of label trees that should be ignored by LabelBOT.
# All "global" label trees should be ignored to avoid tainted results.
LABELBOT_IGNORE_LABEL_TREES=

CACHE_STORE="redis"
SESSION_DRIVER="redis"
SESSION_LIFETIME=120
QUEUE_CONNECTION="redis"

REDIS_HOST="cache"
REDIS_PASSWORD=null
REDIS_PORT=6379

# see config/mail.php for what drivers are available
MAIL_MAILER="smtp"
MAIL_HOST="smtp.mailtrap.io"
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_ADDRESS="noreply@example.com"
